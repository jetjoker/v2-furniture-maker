{
  "connection": "default",
  "collectionName": "order",
  "info": {
    "name": "order",
    "description": ""
  },
  "options": {
    "timestamps": true
  },
  "attributes": {
    "totalPrice": {
      "type": "integer"
    },
    "shippingAddress": {
      "type": "string"
    },
    "shippingDate": {
      "type": "date"
    },
    "shippingStatus": {
      "type": "string"
    },
    "note": {
      "type": "text"
    },
    "orderDetails": {
      "collection": "orderdetail",
      "via": "order"
    },
    "user": {
      "model": "user",
      "plugin": "users-permissions"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "processing",
        "confirmed",
        "cancel",
        "done"
      ]
    },
    "totalDiscount": {
      "type": "integer"
    }
  }
}