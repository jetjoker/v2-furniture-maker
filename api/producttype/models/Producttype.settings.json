{
  "connection": "default",
  "collectionName": "producttype",
  "info": {
    "name": "producttype",
    "description": ""
  },
  "options": {
    "timestamps": true
  },
  "attributes": {
    "name": {
      "unique": true,
      "type": "string",
      "required": true
    },
    "productTypeGroup": {
      "model": "producttypegroup",
      "via": "productTypes"
    },
    "thumbnail": {
      "model": "file",
      "via": "related",
      "plugin": "upload"
    },
    "designs": {
      "collection": "design",
      "via": "productType"
    },
    "discountByQuantities": {
      "collection": "discountbyquantity",
      "via": "productType"
    },
    "orderDetails": {
      "collection": "orderdetail",
      "via": "productType"
    },
    "options": {
      "type": "json",
      "required": true
    },
    "size": {
      "type": "string"
    },
    "volume": {
      "type": "decimal"
    }
  }
}